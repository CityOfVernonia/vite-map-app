{
  "name": "vite-map-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint",
    "postinstall": "node ./scripts/postinstall.js",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "serve": "npm run build && vite preview",
    "test": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/COV-GIS/vite-map-app.git"
  },
  "author": "COV-GIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/COV-GIS/vite-map-app/issues"
  },
  "homepage": "https://github.com/COV-GIS/vite-map-app#readme",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "chalk": "^5.6.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "tslib": "^2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9"
  },
  "dependencies": {
    "@vernonia/core": "^5.1.18"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
